    "version": "1.1"
  },
  "attributes": {
    "processes": [
      "p p > go dl",
      "p p > go dr",
      "p p > go ul",
      "p p > go ur",
      "p p > go sl",
      "p p > go sr",
      "p p > go cl",
      "p p > go cr",
      "p p > go bl",
      "p p > go br"
    ],
    "collider": "pp",
    "ecm": "13TeV"
